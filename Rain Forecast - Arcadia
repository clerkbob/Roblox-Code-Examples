	else 
	
	wait(120)
		
	end
	
	
		
	elseif rand == 2 then --Continue Raining/Clear
		
		if TownM.RainDay.Value == false then
		
		
	wait(120)
	
		elseif TownM.RainDay.Value == true then	
			
			wait(120)
	
