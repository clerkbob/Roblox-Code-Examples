	if rand == 1 then --Rain
	if TownM.RainDay.Value == false then
		

		TownM.RainDay.Value = true
		local Torchythingehz = game.Lighting:FindFirstChild("RainClouds"):clone()
	Torchythingehz.Parent = game.Workspace
