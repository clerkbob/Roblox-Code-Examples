	game.Workspace.RainClouds.Part3.ParticleEmitter1.Enabled = true
	game.Workspace.RainClouds.Part3.ParticleEmitter2.Enabled = true
	game.Workspace.RainClouds.Part4.ParticleEmitter1.Enabled = true
